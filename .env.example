# Frontend Configuration
# Copy this file to .env.local and fill in the values

# Backend API URL (should match backend PORT)
NEXT_PUBLIC_API_URL="http://localhost:5001"

# WebSocket URL (should match backend PORT)
NEXT_PUBLIC_SOCKET_URL="http://localhost:5001"

# NextAuth Configuration (REQUIRED)
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="generate-a-random-secret-here"

# Admin User (default credentials - change in production!)
ADMIN_USERNAME="admin"
ADMIN_PASSWORD="changeme"
ADMIN_EMAIL="admin@example.com"
ADMIN_NAME="Admin"

# Agent Coordination
DASHBOARD_AGENT_TOKEN="your-agent-token-here"
OPENCLAW_WEBHOOK_TOKEN="your-webhook-token-here"

# Agent Webhook URLs (optional - defaults to localhost)
# AGENT_WEBHOOK_JARVIS="http://localhost:18789"
# AGENT_WEBHOOK_LUX="http://localhost:18789"

# Redis (optional - for real-time features)
# REDIS_URL="redis://localhost:6379"

# Backend Port (optional)
# BACKEND_PORT=5001
